<?xml version="1.0"?>
<package >
  <metadata>
    <id>WavesPlatform.WavesCS</id>
    <version>1.1.0.7</version>
    <title>WavesCS library</title>
    <authors>alexkof, abegicheva</authors>
    <owners>alexkof, abegicheva, wavesplatform</owners>
    <licenseUrl>https://github.com/wavesplatform/WavesCS/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/wavesplatform/WavesCS</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
      A library for interacting with the Waves blockchain. Supports node interaction, offline transaction signing, Matcher orders, and creating addresses and keys.
    </description>
    <dependencies>
      <dependency id="Newtonsoft.Json" version="11.0.2" />
      <dependency id="PCLCrypto" version="2.1.32-alpha"/>
      </dependencies>
    <references>
      <reference file="Blake2Sharp.dll" />
      <reference file="curve25519.dll" />
      <reference file="HashLib.dll" />
      <reference file="WavesCS.dll" />
    </references>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System"  />
      <frameworkAssembly assemblyName="System.Core" targetFramework="net45" />
      <frameworkAssembly assemblyName="System.Data" targetFramework="net45" />
      <frameworkAssembly assemblyName="System.Xml" targetFramework="net45" />
      <frameworkAssembly assemblyName="System.Xml.Linq" targetFramework="net45" />
      <frameworkAssembly assemblyName="System.Numerics" targetFramework="net45" />
      <frameworkAssembly assemblyName="System.IO.Compression.FileSystem" />
      <frameworkAssembly assemblyName="System.Runtime.Serialization" />    
    </frameworkAssemblies>
    <releaseNotes>initial release</releaseNotes>
    <copyright>Copyright 2018</copyright>
    <tags>wavescs wavesplatform waves</tags>
  </metadata>
</package>